<%- include('partials/_header.ejs')%>
    <%- include('partials/_navigation.ejs')%>
<div class="all-title-box">
    <div class="container text-center">
        <h1>
            <span class="m_1">ADMIN</span>
        </h1>
    </div>
</div>
<div id="overviews" class="section wb">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 blog-post-single">
                <div class="blog-author">
                    <div class="author-bio">
                        <h3 class="author_name"><a href="#"> Courses Enrolled by: </a></h3>
                    </div>
                </div>
            </div><!-- end col -->
        </div><!-- end row -->
        <div class="row mt-5">
            <h2>Users</h2>
            <table class="table">
                <thead>
                  <tr>
                    <th scope="col">username</th>
                    <th scope="col">Email</th>
                    
                  </tr>
                </thead>
                <tbody>
                    <%for (let i=0;i<sjobs.students.length;i++){%>
                        
                  <tr>
                   
                    
                    <td><%=sjobs.students[i].username%></td>
                    <td><%=sjobs.students[i].email%></td>
                    <td><form method="GET" action="/users/<%=sjobs.students[i]._id%>">
                        <button class="btn btn-danger">Inspect</button></form></td>
                    
                  </tr>
                  <%}%>
                  
           
                </tbody>
              </table>
        </div><!-- end row -->
        
    </div><!-- end container -->
</div><!-- end section -->

<%- include('partials/_footer.ejs')%>
    